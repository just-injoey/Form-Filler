{
    "manifest_version": 2,
    "name": "Form Filler",
    "description": "Facebook Filler",
    "version": "0.0.1",
    "browser_action": {
        "default_title": "Click me",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}